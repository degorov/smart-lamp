{
  "name": "smart-lamp",
  "version": "0.2.1",
  "description": "Умеренно умный RGB-светильник",
  "keywords": [
    "esp32",
    "led-strips",
    "micropython",
    "javascript",
    "react",
    "material-ui"
  ],
  "author": "Dmitry Egorov <mail@degorov.com>",
  "homepage": "http://lamp.degorov.com/",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "start": "webpack serve --hot --mode development",
    "predeploy": "npm run build-prod",
    "deploy": "gh-pages -d dist",
    "serve": "http-server dist -c-1",
    "android": "npm run build-prod && npx cordova-res android --skip-config --copy && npx cap run android",
    "android-studio": "npm run build-prod && npx cap sync && npx cordova-res android --skip-config --copy && npx cap open android"
  },
  "dependencies": {
    "@capacitor-community/http": "^1.3.0",
    "@capacitor/android": "^3.2.4",
    "@capacitor/core": "^3.2.4",
    "@fontsource/roboto": "^4.2.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@capacitor/cli": "^3.2.4",
    "@hot-loader/react-dom": "^17.0.1+4.13.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cordova-res": "^0.15.3",
    "css-loader": "^5.1.0",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.2.0",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^1.3.9",
    "prettier": "^2.2.1",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.3.0"
  }
}
